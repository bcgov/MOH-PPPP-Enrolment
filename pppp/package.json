{
  "name": "pppp",
  "version": "1.1.9",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "serve": "vite preview --host",
    "test": "vitest",
    "test:unit": "vitest --coverage",
    "test:e2e": "start-server-and-test 'vite --host --port 5173' 5173 'cypress open --e2e --config baseUrl=http://localhost:5173/pppp/ --env environment=local'",
    "test:e2e:headless": "start-server-and-test 'vite --host --port 5173' 5173 'cypress run --browser chrome --config baseUrl=http://localhost:5173/pppp/ --env environment=local'",
    "test:e2e:dev": "cypress open --e2e --config baseUrl=http://dev.my.gov.bc.ca/pppp/ --env environment=dev",
    "test:e2e:test": "cypress open --e2e --config baseUrl=http://test.my.gov.bc.ca/pppp/ --env environment=test",
    "lint": "eslint .",
    "format": "prettier . --write"
  },
  "dependencies": {
    "@bcgov/bootstrap-v5-theme": "^1.0.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/vue-fontawesome": "^3.1.1",
    "@vuelidate/core": "^2.0.1",
    "@vuelidate/validators": "^2.0.0-alpha.29",
    "axios": "^1.11.0",
    "common-lib-vue": "^3.1.1-alpha.6",
    "core-js": "^3.38.1",
    "date-fns": "^3.6.0",
    "uuid": "^8.3.2",
    "vue": "^3.5.18",
    "vue-router": "^4.0.15",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.17.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/eslint-plugin": "^1.3.4",
    "@vue/compiler-sfc": "^3.1.0",
    "@vue/test-utils": "^2.0.2",
    "cypress": "^14.5.4",
    "eslint": "^9.33.0",
    "eslint-plugin-compat": "^6.0.2",
    "eslint-plugin-cypress": "^5.1.1",
    "eslint-plugin-vue": "^10.4.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "start-server-and-test": "^2.0.12",
    "vite": "^7.1.3",
    "vitest": "^3.2.4"
  }
}
